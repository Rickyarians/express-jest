{
    "env": {
        "browser": true,
        "es2021": true,
        "amd": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-useless-constructor": "error",
        "camelcase": "off",
        "no-unused-vars": "warn",
        "no-const-assign": "error",
        "no-use-before-define": "error",
        "no-self-compare": "warn"
        // "callback-return": [ "warn`", [ "callback", "next" ]]
    }
}
